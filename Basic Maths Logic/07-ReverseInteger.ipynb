   "source": [
    "n = 12345\n",
    "is_negative = False\n",
    "if n < 0:\n",
    "    is_negative = True\n",
    "num = abs(n)\n",
    "ans = 0\n",
    "while num > 0:\n",
    "    last_digit = num % 10\n",
    "    ans = (ans * 10) + last_digit\n",
    "    num = num // 10\n",
    "\n",
    "if ans < -2**31 or ans > 2**31 - 1:\n",
    "    print(0)\n",
    "\n",
    "if is_negative:\n",
    "    ans = -ans\n",
    "\n",
    "print(ans)"
   ]
  }
